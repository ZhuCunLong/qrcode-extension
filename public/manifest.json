{
  "name": "qrcode-extension",
  "description": "Using ReactJS to build a Chrome Extension",
  "version": "1.0.0",
  "manifest_version": 3,
  "icons": {
    "16": "img/icon.png",
    "128": "img/icon.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "img/icon.png"
  },
  "permissions": [
    "tabs",
    "storage"
  ],
  "host_permissions": [
    "*://*/*"
  ]
}